	<!-- WindowTitleBarButtonStyle -->
	<Style x:Key="{x:Static ribbonThemes:RibbonCommonDictionary.WindowTitleBarButtonStyleKey}" TargetType="{x:Type ribbonControlsPrimitives:RibbonWindowTitleBarButton}">
		<Setter Property="BackgroundActiveHover" Value="{DynamicResource {x:Static ribbonThemes:RibbonCommonDictionary.WindowTitleBarButtonBackgroundActiveHoverBrushKey}}" />
		<Setter Property="BackgroundActiveHoverHighlight" Value="{DynamicResource {x:Static ribbonThemes:RibbonCommonDictionary.WindowTitleBarButtonBackgroundActiveHoverHighlightBrushKey}}" />
		<Setter Property="BorderActiveHoverBrush" Value="{DynamicResource {x:Static ribbonThemes:RibbonCommonDictionary.WindowTitleBarButtonOuterBorderActiveHoverBrushKey}}" />
		<Setter Property="ForegroundActiveHover" Value="{DynamicResource {x:Static ribbonThemes:RibbonCommonDictionary.WindowTitleBarButtonForegroundActiveHoverBrushKey}}" />
		<Setter Property="BackgroundActivePressed" Value="{DynamicResource {x:Static ribbonThemes:RibbonCommonDictionary.WindowTitleBarButtonBackgroundActivePressedBrushKey}}" />
		<Setter Property="BackgroundActivePressedHighlight" Value="{DynamicResource {x:Static ribbonThemes:RibbonCommonDictionary.WindowTitleBarButtonBackgroundActivePressedHighlightBrushKey}}" />
		<Setter Property="BorderActivePressedBrush" Value="{DynamicResource {x:Static ribbonThemes:RibbonCommonDictionary.WindowTitleBarButtonOuterBorderActivePressedBrushKey}}" />
		<Setter Property="ForegroundActivePressed" Value="{DynamicResource {x:Static ribbonThemes:RibbonCommonDictionary.WindowTitleBarButtonForegroundActivePressedBrushKey}}" />
		<Setter Property="Height" Value="25" />
		<Setter Property="Margin" Value="0,0,0,-3" />
		<Setter Property="Width" Value="25" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ribbonControlsPrimitives:RibbonWindowTitleBarButton}">
									
					<Grid Background="Transparent" SnapsToDevicePixels="True">
						<Grid.RowDefinitions>
							<RowDefinition Height="*" />
							<RowDefinition Height="*" />
						</Grid.RowDefinitions>
						
						<Rectangle x:Name="upperBackground" Margin="1,1,1,0" />
						<Rectangle x:Name="lowerBackground" Grid.Row="1" Margin="1,0,1,1" />
						<Border x:Name="innerBorder" Grid.RowSpan="2" Margin="1" BorderBrush="Transparent" BorderThickness="1" CornerRadius="1" SnapsToDevicePixels="True">
							<Canvas x:Name="glyph" Margin="{TemplateBinding Padding}" Width="9" Height="9" HorizontalAlignment="Center" VerticalAlignment="Center" SnapsToDevicePixels="True">
								<Path x:Name="glyphPath1" StrokeThickness="1" Stroke="#9B9B9B" StrokeStartLineCap="Square" StrokeEndLineCap="Square" SnapsToDevicePixels="True" />
								<Path x:Name="glyphPath3" StrokeThickness="1" Stroke="#F8F9FA" StrokeStartLineCap="Square" StrokeEndLineCap="Square" SnapsToDevicePixels="True" />
								<Path x:Name="glyphPath2" StrokeThickness="1" Stroke="#B8C2D0" StrokeStartLineCap="Square" StrokeEndLineCap="Square" SnapsToDevicePixels="True" />
							</Canvas>
						</Border>
						<Border x:Name="outerBorder" Grid.RowSpan="2" BorderBrush="Transparent" BorderThickness="1" CornerRadius="2" SnapsToDevicePixels="True" />
					</Grid>

					<ControlTemplate.Triggers>
						<Trigger Property="Command" Value="{x:Static ribbonInput:RibbonCommands.MinimizeWindow}">
							<Setter TargetName="glyph" Property="Width" Value="7" />
							<Setter TargetName="glyphPath1" Property="Data" Value="M 0.5,6.5 L 6.5,6.5" />
							<Setter TargetName="glyphPath2" Property="Data" Value="M 0.5,7.5 L 6.5,7.5" />
							<Setter TargetName="glyphPath3" Property="Data" Value="M 0.5,8.5 L 6.5,8.5" />
						</Trigger>
						<Trigger Property="Command" Value="{x:Static ribbonInput:RibbonCommands.MaximizeWindow}">
							<Setter TargetName="glyphPath1" Property="Data" Value="M 0.5,1.5 L 8.5,1.5" />
							<Setter TargetName="glyphPath2" Property="Data" Value="M 0.5,2.5 L 8.5,2.5 L 8.5,7.5 L 0.5,7.5 L 0.5,3.5 L 8.5,3.5" />
							<Setter TargetName="glyphPath3" Property="Data" Value="M 1.5,4.5 L 7.5,4.5 M 0.5,8.5 L 8.5,8.5" />
						</Trigger>
						<Trigger Property="Command" Value="{x:Static ribbonInput:RibbonCommands.RestoreWindow}">
							<Setter TargetName="glyphPath1" Property="Data" Value="M 0.5,2.5 L 6.5,2.5 M 8.5,1.5 L 2.5,1.5" />
							<Setter TargetName="glyphPath2" Property="Data" Value="M 6.5,6.5 L 6.5,3.5 L 0.5,3.5 L 0.5,7.5 L 6.5,7.5 L 6.5,6.5 L 8.5,6.5 L 8.5,2.5 L 7.5,2.5" />
							<Setter TargetName="glyphPath3" Property="Data" Value="M 6.5,7.5 L 6.5,4.5 L 0.5,4.5 L 0.5,8.5 L 6.5,8.5 L 6.5,7.5 L 8.5,7.5 L 8.5,3.5 L 7.5,3.5" />
						</Trigger>
						<Trigger Property="Command" Value="{x:Static ribbonInput:RibbonCommands.CloseWindow}">
							<Setter TargetName="glyph" Property="Width" Value="11" />
							<Setter TargetName="glyphPath1" Property="Data" Value="M 1.5,0.5 L 3.5,0.5 L 5.5,2.5 L 7.5,0.5 L 9.5,0.5" />
							<Setter TargetName="glyphPath2" Property="Data" Value="M 2.5,1.5 L 3.5,1.5 L 5.5,3.5 L 7.5,1.5 L 8.5,1.5 L 6.5,3.5 L 6.5,4.5 L 9.5,7.5 L 7.5,7.5 L 5.5,5.5 L 3.5,7.5 L 1.5,7.5 L 4.5,4.5 L 4.5,3.5 Z" />
							<Setter TargetName="glyphPath3" Property="Data" Value="M 0.5,8.5 L 3.5,8.5 L 5.5,6.5 L 7.5,8.5 L 10.5,8.5" />
							<Setter TargetName="glyphPath2" Property="Fill" Value="{Binding RelativeSource={RelativeSource Self}, Path=Stroke}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter TargetName="glyph" Property="Opacity" Value="0.4" />
						</Trigger>
						<Trigger Property="IsActive" Value="True">
							<Setter TargetName="glyphPath1" Property="Stroke" Value="#656565" />
							<Setter TargetName="glyphPath2" Property="Stroke" Value="#829AB9" />
							<Setter TargetName="glyphPath3" Property="Stroke" Value="#F8F9FA" />
						</Trigger>
						
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsEnabled" Value="True" />
								<Condition Property="IsMouseOver" Value="True" />
								<Condition Property="IsPressed" Value="False" />
							</MultiTrigger.Conditions>
							<Setter TargetName="outerBorder" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderActiveHoverBrush}" />
							<Setter TargetName="upperBackground" Property="Fill" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BackgroundActiveHoverHighlight}" />
							<Setter TargetName="lowerBackground" Property="Fill" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BackgroundActiveHover}" />
							<Setter TargetName="glyphPath1" Property="Stroke" Value="#656565" />
							<Setter TargetName="glyphPath2" Property="Stroke" Value="#829AB9" />
							<Setter TargetName="glyphPath3" Property="Stroke" Value="#F8F9FA" />
						</MultiTrigger>
						
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsEnabled" Value="True" />
								<Condition Property="IsPressed" Value="True" />
							</MultiTrigger.Conditions>
							<Setter TargetName="outerBorder" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderActivePressedBrush}" />
							<Setter TargetName="upperBackground" Property="Fill" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BackgroundActivePressedHighlight}" />
							<Setter TargetName="lowerBackground" Property="Fill" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BackgroundActivePressed}" />
							<Setter TargetName="glyphPath1" Property="Stroke" Value="#656565" />
							<Setter TargetName="glyphPath2" Property="Stroke" Value="#829AB9" />
							<Setter TargetName="glyphPath3" Property="Stroke" Value="#F8F9FA" />
						</MultiTrigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
			
	<!-- WindowTitleBarAlternateButtonStyle -->
