    <!-- WindowTitleBarButtonStyle -->
    <Style x:Key="{x:Static dockingThemes:DockingCommonDictionary.WindowTitleBarButtonStyleKey}" TargetType="{x:Type dockingPrimitives:TitleBarButton}">
