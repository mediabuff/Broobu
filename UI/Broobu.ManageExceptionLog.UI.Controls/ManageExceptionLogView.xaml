<ui:ViewUserControl x:Class="Pms.ManageExceptionLog.UI.Controls.ManageExceptionLogView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ui="clr-namespace:Pms.Framework.UI;assembly=Pms.Framework.UI"
             xmlns:dock="http://schemas.actiprosoftware.com/winfx/xaml/docking"
             xmlns:me="clr-namespace:Pms.ManageExceptionLog.UI.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    
    <Grid>
            
        <dock:DockSite>
            <dock:SplitContainer>
                <dock:Workspace>
                    <me:ExceptionLogListView HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                </dock:Workspace>
                <dock:DockingWindow Title="Properties">
                    
                </dock:DockingWindow>
            </dock:SplitContainer>
        </dock:DockSite>
    </Grid>
</ui:ViewUserControl>
